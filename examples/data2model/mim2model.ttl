		PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
		PREFIX mim: <http://bp4mc2.org/def/mim#>
		PREFIX sh: <http://www.w3.org/ns/shacl#>
		INSERT {
			GRAPH <http://localhost:8080/data2model/container/model> {
